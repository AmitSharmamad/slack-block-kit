{
    "name": "slack-blockify",
    "version": "2.1.1",
    "description": "A framework for Slack Block Kit Interactive actions",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/AmitSharmamad/slack-block-kit.git"
    },
    "bugs": {
        "email": "amitsharmamad@gmail.com",
        "url": "https://github.com/AmitSharmamad/slack-block-kit/issues"
    },
    "author": "Amit Madgula",
    "contributors": [
        {
            "email": "amitsharmamad@gmail.com",
            "name": "amit madgula"
        }
    ],
    "maintainers": [
        {
            "name": "amit madgula",
            "email": "amitsharmamad@gmail.com"
        }
    ],
    "keywords": [
        "slack-blockify",
        "slack-block-kit",
        "slack-types",
        "slack-interactive-actions",
        "typescript"
    ],
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "dev": "nodemon --exec ts-node src/app.ts",
        "rm-ts-files": "rm -r {core,decorators,test} && rm index.ts",
        "rm-extra": "rm -r {dist,node_modules} && npm run rm-tests && rm package-lock.json",
        "rm-tests": "rm -r test",
        "publish": "npm i && tsc && npm run rm-ts-files && mv dist/* . && npm run rm-extra",
        "test": "mocha -r ts-node/register test/**/*.test.ts",
        "test:debug": "nodemon --exec mocha -r ts-node/register test/**/*.test.ts"
    },
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/node": "^13.11.0",
        "nodemon": "^2.0.2",
        "ts-node": "^8.8.1"
    },
    "dependencies": {
        "chai": "^4.2.0",
        "reflect-metadata": "^0.1.13",
        "slack-blockify-types": "^1.0.0",
        "typescript": "^3.8.3"
    }
}
